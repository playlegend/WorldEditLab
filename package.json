{
  "name": "worldeditlab",
  "version": "1.0.0",
  "description": "A fancy and simple UI for up- & downloading schematics and heightmaps",
  "main": "dist/app.js",
  "scripts": {
    "start": "node .",
    "serve": "concurrently -r \"yarn serve:tsc\" \"yarn serve:css\" \"yarn serve:js\"",
    "serve:css": "postcss -w assets/styles/tailwind.css -o public/styles/style.css",
    "serve:tsc": "tsc-watch -p ./src --onSuccess \"node .\"",
    "serve:js": "webpack --watch",
    "build": "yarn build:tsc && yarn build:js && yarn build:css",
    "build:tsc": "tsc -p ./src",
    "build:css": "postcss assets/styles/tailwind.css -o public/styles/style.css",
    "build:js": "webpack",
    "lint": "yarn lint:ts && yarn lint:css",
    "lint:ts": "npx eslint .",
    "lint:css": "npx stylelint \"assets/**/*.css\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/playlegend/WorldEditLab.git"
  },
  "author": "playlegend.net",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/playlegend/WorldEditLab/issues"
  },
  "homepage": "https://github.com/playlegend/WorldEditLab#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.16",
    "@babel/preset-env": "^7.12.16",
    "@types/bcrypt": "^3.0.0",
    "@types/ejs": "^3.0.5",
    "@types/express": "^4.17.11",
    "@types/express-fileupload": "^1.1.6",
    "@types/express-session": "^1.17.3",
    "@types/passport": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^8.2.2",
    "concurrently": "^5.3.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.1",
    "postcss": "^8.2.6",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "stylelint": "^13.10.0",
    "stylelint-config-standard": "^20.0.0",
    "tailwindcss": "^2.0.3",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.1.5",
    "webpack-cli": "^4.5.0",
    "webpack": "^5.21.2"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-session": "^1.17.1",
    "passport": "^0.4.1",
    "passport-custom": "^1.1.1",
    "sequelize": "^6.5.0",
    "sqlite3": "5.0.0"
  }
}
